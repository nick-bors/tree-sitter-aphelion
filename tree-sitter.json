{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "aphelion",
      "camelcase": "Aphelion",
      "title": "Aphelion",
      "scope": "source.aphelion",
      "file-types": [
        ".inc",
        ".S",
        ".aphel",
        ".asm",
        ".s",
        ".aphel64"
      ],
      "injection-regex": "^aphelion$",
      "class-name": "TreeSitterAphelion"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree sitter grammar for Aphel64 assembly",
    "authors": [
      {
        "name": "Nick Bors",
        "email": "nick@nickbors.cc",
        "url": "https://nickbors.cc"
      }
    ],
    "links": {
      "repository": "https://github.com/nick-bors/tree-sitter-aphelion.git",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": true
  }
}